<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kite.joco.recycchoosersearchp1">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="lelt_pro_db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.kite.joco.recycchoosersearchp1.db" />

        <activity
            android:name=".MainActivity"
            android:configChanges="keyboardHidden|orientation|navigation"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Szamolo"
            android:configChanges="keyboardHidden|orientation|navigation"
            android:screenOrientation="landscape"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kite.joco.recycchoosersearchp1.MainActivity" />
        </activity>

        <activity android:name=".Ellenorzo"
            android:configChanges="keyboardHidden|orientation|navigation"
            android:screenOrientation="landscape"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kite.joco.recycchoosersearchp1.MainActivity" />
        </activity>
    </application>

</manifest>

